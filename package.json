{
    "name": "remote",
    "version": "0.0.1",
    "scripts": {
      "ng": "ng",
      "gv": "genversion --esm --semi src/lib/version.ts",
      "start": "ng serve --port 4201",
      "clean": "npx rimraf ./dist",
      "build": "npm run clean && npm run gv && ng build",
      "serve": "ng serve --port 4201 --host 0.0.0.0 --disable-host-check",
      "watch": "ng build --watch --configuration development",
      "test": "ng test --browsers=Chrome --watch=false --code-coverage",
      "test-ci": "ng test --browsers=ChromeHeadlessNoSandbox --watch=false --code-coverage",
      "lint": "ng lint",
      "lint-fix": "ng lint --fix",
      "packagr": "npm run clean && npm run gv && ng-packagr --project ng-package.json",
      "compodoc": "compodoc",
      "generate-docs": "compodoc -p tsconfig.doc.json",
      "serve-docs": "compodoc -s tsconfig.doc.json",
      "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
    },
    "private": false,
    "dependencies": {
      "tslib": "^2.3.0"
    },
    "peerDependencies": {
      "@angular-architects/module-federation": "^17.0.8",
      "@angular/animations": "^17.3.0",
      "@angular/common": "^17.3.0",
      "@angular/compiler": "^17.3.0",
      "@angular/core": "^17.3.0",
      "@angular/elements": "^17.3.5",
      "@angular/forms": "^17.3.0",
      "@angular/platform-browser": "^17.3.0",
      "@angular/platform-browser-dynamic": "^17.3.0",
      "@angular/router": "^17.3.0",
      "@astrouxds/angular": "^7.21.0",
      "@astrouxds/astro-web-components": "^7.21.0",
      "cesium": "^1.115.0",
      "rxjs": "~7.8.0",
      "socket.io-client": "^4.7.5",
      "zone.js": "~0.14.3",
      "html-loader": "^5.0.0",
      "html-webpack-plugin": "^5.6.0",
      "js-loader": "^0.1.1",
      "mini-css-extract-plugin": "^2.9.0",
      "postcss-loader": "^8.1.1",
      "sass-loader": "^14.2.1",
      "style-loader": "^4.0.0",
      "webpack": "^5.91.0",
      "webpack-dev-server": "^4.15.1"
    },
    "devDependencies": {
      "@angular-architects/module-federation-tools": "^17.0.8",
      "@angular-devkit/build-angular": "^17.3.0",
      "@angular-eslint/builder": "17.3.0",
      "@angular-eslint/eslint-plugin": "17.3.0",
      "@angular-eslint/eslint-plugin-template": "17.3.0",
      "@angular-eslint/schematics": "17.3.0",
      "@angular-eslint/template-parser": "17.3.0",
      "@angular/cli": "^17.3.0",
      "@angular/compiler-cli": "^17.3.0",
      "@compodoc/compodoc": "^1.1.23",
      "@types/jasmine": "~5.1.0",
      "@typescript-eslint/eslint-plugin": "7.2.0",
      "@typescript-eslint/parser": "7.2.0",
      "copy-webpack-plugin": "^12.0.2",
      "eslint": "^8.57.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-import-resolver-typescript": "^3.6.1",
      "eslint-plugin-import": "^2.29.1",
      "eslint-plugin-jsdoc": "^48.2.2",
      "eslint-plugin-prettier": "^5.1.3",
      "eslint-plugin-sonarjs": "^0.25.0",
      "genversion": "^3.2.0",
      "jasmine": "^5.1.0",
      "jasmine-core": "~5.1.0",
      "karma": "~6.4.0",
      "karma-chrome-launcher": "~3.2.0",
      "karma-coverage": "~2.2.0",
      "karma-coverage-istanbul-reporter": "^3.0.3",
      "karma-jasmine": "~5.1.0",
      "karma-jasmine-html-reporter": "~2.1.0",
      "ng-packagr": "^17.3.0",
      "ngx-build-plus": "^17.0.0",
      "prettier": "^3.2.5",
      "prettier-eslint": "^16.3.0",
      "strip-pragma-loader": "^1.0.1",
      "typescript": "~5.4.2"
    }
  }
  